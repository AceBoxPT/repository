<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.AceBoxWIZARD" name="AceBox Wizard" version="0.7" provider-name="AceBox">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">AceBox Wizard</summary>
    <description lang="en">AceBox Wizard allows you to install the latest AceBox Builds.</description>
    <platform>all</platform>
  </extension>
</addon>